	font-family: 'Lato', Helevetica, Arial, sans-serif;
	font-weight: 400;
	color: #8A8A8A;
}
#wrapper {
	margin: 0 auto;
	width: 1020px;
	position: relative;
	z-index: 2;
}

#logo {
	width: 116px;
	height: 57px;
	display: block;
	background: url('../images/logo.png') no-repeat;
	margin:0 0 20px 0;
