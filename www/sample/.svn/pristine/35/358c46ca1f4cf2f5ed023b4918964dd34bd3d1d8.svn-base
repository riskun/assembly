/* HYPERLINKS begin */
a {
	color: #00aeef;
	text-decoration: none;
	outline:none;
}
a:hover {
	text-decoration: none;
}
/* HYPERLINKS end */
/* LIST STYLES begin */
ul, ol, dl {
	margin-left: 0;
	padding-left: 0;
	font-size: 0.875em;
}
ul, ol {
	margin-left: 20px;
}
ul li, ol li {
	margin-bottom: 0px;
}
ul li ul, ol li ul {
	margin: 5px 0 5px 15px;
}
::selection {
	background: #BCE6F7; /* Safari */
	}
::-moz-selection {
	background: #BCE6F7; /* Firefox */
}
/* LIST STYLES end */
/* FROM FIELDS begin */
fieldset {
	border: 1px solid #f9f9f9;
}
label {
	font-size: 1em;
	margin-bottom: 10px;
	display: block;
}
input[type="text"],input[type="number"],input[type="file"], input[type="password"], input[type="checkbox"], input[type="radio"], select, textarea {
	padding: 5px;
	border: 1px solid #DDDDDD;
	/*Applying CSS3 gradient*/
	background: -moz-linear-gradient(center top , #FFFFFF,  #f9f9f9 1px, #FFFFFF 20px);
	background: -webkit-gradient(linear, left top, left 20, from(#FFFFFF), color-stop(5%, #f9f9f9) to(#FFFFFF));
	filter: progid :DXImageTransform.Microsoft.gradient(startColorstr='#FBFBFB', endColorstr='#FFFFFF');
	/*Applying CSS 3radius*/
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	/*Applying CSS3 box shadow*/
	-moz-box-shadow: 0 0 2px #DDDDDD;
	-webkit-box-shadow: 0 0 2px #DDDDDD;
	box-shadow: 0 0 2px #DDDDDD;
	color: #999;
	font-size:0.688em;
	outline:none;
}
input[type="text"]:hover {
	border: 1px solid #cccccc;
}
input[type="text"]:focus, input[type="password"]:focus, select:focus, textarea:focus, input[type="checkbox"]:focus, input[type="radio"]:focus, input[type="file"]:focus {
	box-shadow: 0 0 2px #00AEEF;
}
input[type="checkbox"], input[type="radio"] {
	position: relative;
	top: 2px;
}
input[type="submit"], input[type="reset"] {
	cursor: pointer;
	border: 0;
	font-family: 'Helvetica', Trebuchet, Arial;
	font-weight: bold;
	color: #ffffff;
	padding: 6px 22px;
	text-transform: uppercase;
	background: #00AEEF;
	background: -moz-linear-gradient(
	top,
	#71cbec 0%,
	#00AEEF);
	background: -webkit-gradient(
	linear, left top, left bottom,
	from(#71cbec),
	to(#00AEEF));
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	text-shadow: 0px -1px 1px rgba(000,000,000,0.2), 0px 1px 0px rgba(255,255,255,0.3);
	text-decoration: none;
}
input[type="submit"]:hover, input[type="reset"]:hover {
	background: #00AEEF
}
/* other buttons begin */
a.button1{
	cursor: pointer;
	border: 0;
	font-family: 'Helvetica', Trebuchet, Arial;
	font-weight: bold;
	color: #00aeef;
	padding: 6px 22px;
	text-transform: lowercase;
	background: #e9e9e9;
	background: -moz-linear-gradient(
	top,
	#fbfbfb 0%,
	#e9e9e9);
	background: -webkit-gradient(
	linear, left top, left bottom,
	from(#fbfbfb),
	to(#e9e9e9));
	border-radius: 15px 15px 15px 15px;	
	-moz-border-radius: 15px 15px 15px 15px;	
	-webkit-border-radius: 15px 15px 15px 15px;	
	font-size:0.813em;
	text-decoration: none;
}
a.button1:hover{
	background: #e9e9e9;
	background: -moz-linear-gradient(
	top,
	#e9e9e9 0%,
	#e9e9e9);
	background: -webkit-gradient(
	linear, left top, left bottom,
	from(#e9e9e9),
	to(#e9e9e9));
}
/* other buttons end */
