/* Content Static Pages */
.content-static-pages .accordionWrapper {
	width: 841px;
	float: right;
}
.content-static-pages aside {
	float: left;
	width: 178px;
	position: relative;
}
/* Home begin */
.sliderContainer
{
	position:relative;
	z-index:0;
	margin-top:30px;
}
.featuredWrap
{
	position:relative;
	height:91px;	
}
.featuredWrap .featuredBox
{
	width:339px;
	height:91px;
	display:block;
	cursor:pointer;
	position:absolute;
	bottom:0;
	left:0;
	z-index:1;
}
.featuredBox .featuredtitle
{
    color: #00aeef;
    display:block;
    float: right;
    font-size: 15px;
    font-weight: bold;
    line-height: 19px;
    position: absolute;
    right: 15px;
    text-align: left;
    text-transform: uppercase;
    top: 27px;
    width: 121px;
    z-index: 2;
}
.featuredBox .featuredicon
{
	background:url('../images/icons-widget-left.gif') no-repeat 0 -332px;
	width:42px;
	height:41px;
	position: absolute;
    bottom: 32px;
    right: 48px;
    z-index: 2;
	display:none;
}
.featuredBox .featuredcopy
{
    bottom: 26px;
    color: #00aeef;
    font-size: 1em;
    font-weight: bold;
    left: 23px;
    line-height: 19px;
    position: absolute;
    width: 201px;
    z-index: 2;
	display:none;
}
.featuredBox img
{
	position:relative;
	z-index:1;
	width:342px !important
}
.bannersWrap
{
	position:relative;
	height:239px;	
	color:#fff;
	margin-bottom:30px;
}
.bannersWrap .bannersBox
{
	width:239px;
	height:239px;
	display:block;
	cursor:pointer;
	float:left;
	position:relative;
	margin-right:10px;
}
.bannersWrap .bannersContent
{
	width:239px;
	height:0;
	position:absolute;
	bottom:0;
	left:0;
	background:url('../images/banners-overlay.png') repeat;
	text-align:center;
	display: table;
}
.bannersContentBox
{
   display: table-cell;
   text-align: center;
   vertical-align: middle;
}
.bannersWrap .bannerstitle
{
	display:none;
	font-size:2.375em;
	font-family: 'Bree Serif', serif;
	margin-bottom:15px;
}
.bannersWrap .bannersCopy
{
    display:none;
	font-size:1em;
}
/* Home end */
/* Events begin */
.table-events {
	font-size: 100%;
	width: 100%;
	margin-bottom:7px;
}
.table-events th {
	text-transform: uppercase;
	font-weight: bold;
	color: #000;
	font-size: 0.750em;
	background: #fff;
	padding: 5px 10px 5px 0px;
	border-bottom: 1px dotted #e7e7e7;
}
.table-events td:first-child {
	text-transform: uppercase;
}
.table-events td {
	padding: 5px 10px 5px 0px;
	text-align: center;
	color: #232323;
	font-size: 0.813em;
	border-bottom: 1px dotted #e7e7e7;
}
.table-events tr:nth-child(odd) {
	background-color: #fff;
}
.table-events tr:nth-child(even) {
	background-color: #fff;
}
.table-events-details th,.table-events-details td
{
	text-align:left;
	border-bottom:0;
}
.table-events-details th:first-child 
{
	width:70px;
}
.events-details-colleft
{
	width:292px;
	float:left;
	margin-right:40px;
}
.events-details-colleft ul
{
	list-style-type:none;
	margin:9px 0 9px 0;
}
.events-details-colleft ul li
{
	float:left;
	display:block;
}
.events-details-colleft ul li a
{
	display:block;
	float:left;
 	margin: 0 9px 4px 0;	
	width:69px;
	height:71px;
	background-color:#bbe9fb;
}
.events-details-colleft ul li a:hover
{
	opacity:0.8
}
.events-details-colleft ul li:nth-child(4n+4) a {  
  	margin-right:0;
}
.events-details-colright
{
	width:375px;
	float:left;
}	
.events-details-colright img
{
	float:right;
}
/* Events end */
/* Support page begin */
.contentsupport
{
	float:left;
	padding-left:0;
}
.latest-news-views
{
	border:1px solid #e7e7e7;
	padding:10px 0 10px 80px;
	margin-bottom:35px;
	position:relative;
}
.askquestion .contentdiv
{
	padding-top:40px;
}
.contentsupport h1
{
	font-size:0.946em;
 	margin-bottom: 0;	
}
.supportSectionHeader
{
	background:#bce6f7;
	padding:10px 0;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;	
	display:block;
}
.supportSectionHeader input {
	background-image: url("../images/icon-search.png");
	background-repeat: no-repeat;
	background-position: 8px center;
	background-color:#fff;
	border: 1px solid #ddd;
	border-radius: 15px 15px 15px 15px;
	margin: 0;
	padding: 5px 14px 5px 24px;
	position: relative;
	text-transform: uppercase;
    top: 0;
	color: #aaa;
	margin-left:80px;
	width:316px;
}
.formSupport input,.formSupport select
{
	margin-bottom:5px;
	display:block;
}
/* Support Page end */
/* Understanding Ingredients page begin */
.understandingredient_sub_title
{
	font-weight:bold;
}
.accordion td
{
	text-align:left !important;
}
/* Understanding Ingredients page end */
