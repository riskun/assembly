/* products details page end */
.content-products aside {
	width: 178px;
	float: left;
}
.content-products aside h1 {
	display: block;
	font-size: 0.81em;
	font-style: italic;
	line-height: 1.38em;
	margin: 6px 0 0 24px;
	width: 127px;
	text-transform: none;
	color: #232323;
	font-family: 'Lato';
}
.content-products aside .skipModule {
	padding-left: 10px;
	position: absolute;
	bottom: 191px;
}

.content-products aside .skipModule input {
	float: left;
}
.content-products aside .skipModule p {
	float: left;
	width: 103px;
	font-size: 0.7em;
	line-height: 1.3em;
}
.list-products-wrap {
	float: left;
	width: 223px;
	position: relative;
	margin-right: 33px;
	z-index: 1;
}

fieldset {
	border:0px;
}

.list-products-wrap input[type="checkbox"]
{
	margin-right:6px;
    position: relative;
    top: 3px;	
}
.list-products-bg {
	z-index: 0;
	background: #bee2ef; /* Old browsers */
	background: -moz-linear-gradient(top, #bee2ef 0%, #d6ecf5 10%, #def0f7 51%, #def0f7 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#bee2ef), color-stop(10%,#d6ecf5), color-stop(51%,#def0f7), color-stop(100%,#def0f7)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #bee2ef 0%,#d6ecf5 10%,#def0f7 51%,#def0f7 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #bee2ef 0%,#d6ecf5 10%,#def0f7 51%,#def0f7 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #bee2ef 0%,#d6ecf5 10%,#def0f7 51%,#def0f7 100%); /* IE10+ */
	background: linear-gradient(to bottom, #bee2ef 0%,#d6ecf5 10%,#def0f7 51%,#def0f7 100%); /* W3C */
	filter: progid :DXImageTransform.Microsoft.gradient( startColorstr='#bee2ef', endColorstr='#def0f7',GradientType=0 ); /* IE6-9 */
	height: 100%;
	width: 223px;
	left: 178px;
	position: absolute;
	top: -1px;
	-moz-box-shadow: 0 0 1px #dfe3e5;
	-webkit-box-shadow: 0 0 5px #dfe3e5;
	box-shadow: 0 0 1px #dfe3e5;
}
.list-products-wrap ul {
	margin-left: 0px;
	padding: 18px 0 37px 0;
	list-style-type: none;
}
.list-products-wrap li {
	margin-bottom: 0;
}
.list-products-wrap ul li a,.list-products-wrap ul li span {
	display: block;
	color: #666666;
	display: block;
	font-size: 0.75em;
	line-height: 0.999em;
	margin-bottom: 0;
	padding: 6px 34px 6px 36px;
	-webkit-transition-property: background, text;
	-webkit-transition-duration: 0.5s, 0.5s;
	-webkit-transition-timing-function: linear, ease-in;
	-moz-transition-property: background, text;
	-moz-transition-duration: 0.5s;
	-moz-transition-timing-function: linear, ease-in;
	-o-transition-property: background, text;
	-o-transition-duration: 0.5s;
	-o-transition-timing-function: linear, ease-in;
	cursor: pointer;
}
.list-products-wrap ul li a:hover,.list-products-wrap ul li span:hover {
	background: #fff url('../images/icon-products-list-arrow.gif') no-repeat 10px center;
	text-transform: uppercase;
	font-weight: bold;
}
.content-products article {
	float: left;
	width: 455px;
	padding-top: 30px;
}
.summaryLeft {
	float: left;
	width: 252px;
	margin-right: 10px;
}
.summaryLeft p {
	font-size: 0.89em;
