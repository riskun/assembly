.priority_item
{
/*	width:200px;*/
	padding:15px !important;
	margin-top:5px;
	border:1px solid #999;
	background-color:#fff !important;
	position:relative;
}

.priority_remove
{
	position:absolute;
	top:0;
	right:0;
	padding:4px;
	background-color:#00BCF2;
	color:#fff;
	cursor:pointer;
	font-size:10px;	
}

